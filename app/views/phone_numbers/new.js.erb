<% if @phone_number.valid? %>
  $('#select').html('<%= escape_javascript render('show', command: "Masukan Kode Coupon") %>');
<% else %>
  $('#notice').remove();
  $('.large-12.columns').prepend('<%= escape_javascript render('shared/notice', alert: "Nomer Telepon tidak terdaftar atau salah") %>');
  $('#select').html('');
<% end %>